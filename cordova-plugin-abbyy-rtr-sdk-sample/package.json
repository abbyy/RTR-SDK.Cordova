{
  "name": "cordova-plugin-abbyy-rtr-sdk-sample",
  "description": "ABBYY Mobile Capture SDK Cordova Plugin Example",
  "version": "1.0.6",
  "displayName": "RTRCordovaSample",
  "cordova": {
    "id": "cordova-plugin-abbyy-rtr-sdk-sample",
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-abbyy-rtr-sdk": {
        "CAMERA_USAGE_DESCRIPTION": "NSCameraUsageDescription",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": "NSPhotoLibraryUsageDescription"
      },
      "cordova-plugin-x-socialsharing": {
        "ANDROID_SUPPORT_V4_VERSION": "24.1.1+",
        "PHOTO_LIBRARY_ADD_USAGE_DESCRIPTION": "This app requires photo library access to function properly.",
        "PHOTO_LIBRARY_USAGE_DESCRIPTION": "This app requires photo library access to function properly."
      },
      "cordova-plugin-androidx-adapter": {}
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abbyysdk/RTR-SDK.Cordova.git"
  },
  "dependencies": {
    "cordova-android": "~8.1.0",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-abbyy-rtr-sdk": "^1.0.15",
    "cordova-plugin-androidx-adapter": "^1.1.0",
    "cordova-plugin-x-socialsharing": "^5.6.5",
    "es6-promise-plugin": "^4.2.2"
  },
  "keywords": [
    "rtr",
    "real time",
    "recognition",
    "data capture",
    "abbyy",
    "mobile sdk",
    "OCR",
    "optical character recognition",
    "ocr-engine",
    "sample",
    "ecosystem:cordova",
    "cordova-ios",
    "cordova-android"
  ],
  "author": "ABBYY Mobile SDK Team",
  "license": "LICENSE.md"
}